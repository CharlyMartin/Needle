<div class="container">
<div class="row">
 <div class="col-md-6 col-md-offset-3">
   <%= simple_form_for @campaign, url: dashboard_campaigns_path, method: :post do |f| %>
    <%= f.error_notification %>
    <%= f.input :title %>
    <%= f.input :batch_size, collection: ["150", "250", "300"] %>
    <%= f.input :date_start, as: :date %>
    <%= f.input :date_end, as: :date %>
    <%= f.input :description %>
    <%= f.input :spec %>
    <%= f.input :price %>
    <%= f.input :category %>
    <%= f.input :gender, collection: ["Mens", "Womens", "Unisex"] %>
    <%= f.input :photo, as: :attachinary %>
     <div class="text-center">
       <%= f.button :submit, class: "btn btn-primary" %>
     </div>
     <% end %>
   </div>
</div>
</div>
